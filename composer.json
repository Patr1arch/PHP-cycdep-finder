{
    "name": "patriarch/php-cycdep-finder",
    "description": "Package to find cyclic dependencies in classes, files, directories and packages in PHP projects",
    "type": "project",
    "require": {
        "php": ">=7.1",
        "nikic/php-parser": "^4.19",
        "ext-json": "*"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Patriarch\\PhpCycdepFinder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Patriarch\\PhpCycdepFinder\\": "test"
        }
    },
    "authors": [
        {
            "name": "Patr1arch"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    }
}
